<div class="container">
  <h1>Restaurants</h1>
  <% @restaurants.each do |restaurant| %>
    <div class="card-restaurant">
      <img src="https://source.unsplash.com/collection/4625880" >
      <div class="card-restaurant-info">
        <%= link_to restaurant.name, restaurant_path(restaurant), class: "restaurant-link" %>
        <p><%= restaurant.category %></p>
      </div>
    </div>
  <% end %>
</div>
